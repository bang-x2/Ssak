   "source": [
    "import string\n",
    "\n",
    "def hashing(alp):\n",
    "    buk = []\n",
    "    h = dict()\n",
    "    for i,v in enumerate(string.ascii_lowercase):\n",
    "        h[v] = i+1\n",
    "    for j,s in enumerate(alp):\n",
    "        buk.append((h[s])*(31**j))\n",
    "    return(sum(buk))"
   ]
